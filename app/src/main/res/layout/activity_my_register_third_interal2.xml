<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/interal2_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="30dp"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/person1"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="占座"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="30dp"/>
        <Button
            android:id="@+id/person2"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="拿快递"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/person1"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person3"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="买饭"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/person2"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person4"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="买东西"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/person3"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person5"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="拼单"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/person4"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="电子产品修理"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person1"
            android:layout_marginLeft="30dp"/>
        <Button
            android:id="@+id/person7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="家具器件组装"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person1"
            android:layout_toRightOf="@id/person6"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person8"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="找同好"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person1"
            android:layout_toRightOf="@id/person7"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="学习作业辅导"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person6"
            android:layout_marginLeft="30dp"/>
        <Button
            android:id="@+id/person10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="技能培训"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person6"
            android:layout_toRightOf="@id/person9"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="选课指南"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person6"
            android:layout_toRightOf="@id/person10"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="考研出国经验"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person9"
            android:layout_marginLeft="30dp"/>
        <Button
            android:id="@+id/person13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="求职经验"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person9"
            android:layout_toRightOf="@id/person12"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="票务转让"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person9"
            android:layout_toRightOf="@id/person13"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="二手闲置"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person14"
            android:layout_marginLeft="30dp"/>
    </RelativeLayout>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@drawable/ic_line"
        android:layout_marginTop="10dp"/>
</LinearLayout>
