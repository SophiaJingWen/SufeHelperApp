<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/register_title_layout"/>
    <TextView
        android:id="@+id/text17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="个人特长"
        android:textSize="25dp"
        android:textColor="@color/colorPrimary"
        android:layout_marginTop="80dp"
        android:layout_centerHorizontal="true"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="@drawable/ic_line"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/text17"/>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/text17">
        <Button
            android:id="@+id/person19"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="占座"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            />
        <Button
            android:id="@+id/person29"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="拿快递"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/person19"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person39"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="买饭"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/person29"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person49"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="买东西"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/person39"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person59"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:text="拼单"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/person49"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person69"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="电子产品修理"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person19"
            />
        <Button
            android:id="@+id/person79"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="家具器件组装"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person19"
            android:layout_toRightOf="@id/person69"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person89"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="找同好"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person19"
            android:layout_toRightOf="@id/person79"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person99"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="学习作业辅导"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person69"
            />
        <Button
            android:id="@+id/person109"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="技能培训"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person69"
            android:layout_toRightOf="@id/person99"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person119"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="周边服务"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person69"
            android:layout_toRightOf="@id/person109"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person129"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="考研出国经验"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person99"
            />
        <Button
            android:id="@+id/person139"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="求职经验"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person99"
            android:layout_toRightOf="@id/person129"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person149"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="票务转让"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person99"
            android:layout_toRightOf="@id/person139"
            android:layout_marginLeft="5dp"/>
        <Button
            android:id="@+id/person159"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="二手闲置"
            android:textSize="15dp"
            android:textColor="@drawable/btn_text"
            android:background="@drawable/btn_click"
            android:layout_marginTop="5dp"
            android:layout_below="@id/person149"
            />
    </RelativeLayout>

    <Button
        android:id="@+id/button_confirm5"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="50dp"
        android:text="完成"
        android:textColor="@color/white"
        android:background="@drawable/confirm_button"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="100dp"
        android:layout_alignParentBottom="true"
        android:textSize="20dp"/>
    <TextView
        android:id="@+id/dot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="○  ○  ○  ○  ●"
        android:textSize="10dp"
        android:textColor="#909090"
        android:background="#00000000"
        android:layout_marginBottom="60dp"
        android:layout_alignParentBottom="true"/>
</RelativeLayout>


